<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml">

	<mx:Metadata>
		[Protected(permission="user",id="b1",controlBy="enabled")]
		[Protected(permission="user",id="b2",controlBy="includeInLayout")]
		[Protected(permission="user",id="b3",controlBy="visible")]
		[Protected(permission="user",id="b4",controlBy="remove")]
		
		[Protected(permission="admin",id="p1",controlBy="enabled")]
		[Protected(permission="admin",id="p2",controlBy="includeInLayout")]
		[Protected(permission="admin",id="p3",controlBy="visible")]
		[Protected(permission="admin",id="p4",controlBy="remove")]
		
