		<mx:VBox id="box1" width="100%" height="100%" label="controlBy enabled">
			<mx:Text text="controlBy enabled" />
		</mx:VBox>
		<mx:VBox id="box2" width="100%" height="100%" label="controlBy includeInLayout">
			<mx:Text text="controlBy includeInLayout" />
