        <mx:Panel id="p1" 
            title="controlBy enabled" 
            backgroundColor="#FF0000"/>
        <mx:Panel id="p2" 
            title="controlBy includeInLayout" 
            backgroundColor="#00FF00"/>
        <mx:Panel id="p3" 
            title="controlBy visible" 
            backgroundColor="#0000FF"/>
        <mx:Panel id="p4" 
            title="controlBy remove" 
            backgroundColor="#0000FF"/>            
